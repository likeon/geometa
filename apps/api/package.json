{
  "name": "api",
  "version": "1.0.50",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --target bun --compile --minify-whitespace --minify-syntax --production --outfile server",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check .",
    "fix": "biome check --write .",
    "type-check": "tsc --noEmit; rm tsconfig.tsbuildinfo",
    "sanity": "bun run format && bun run fix && bun run type-check"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.4.1",
    "@elysiajs/server-timing": "^1.4.0",
    "@elysiajs/swagger": "^1.3.1",
    "@sentry/bun": "^10.22.0",
    "async-retry": "^1.3.3",
    "aws4fetch": "^1.0.20",
    "cmd-ts": "^0.14.3",
    "drizzle-orm": "^0.44.7",
    "elysia": "^1.4.15",
    "jose": "^6.1.0",
    "memoize-one": "^6.0.0",
    "postgres": "^3.4.7",
    "rehype-external-links": "^3.0.0",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "remeda": "^2.32.0",
    "sharp": "^0.34.4",
    "unified": "^11.0.5"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.0",
    "@types/async-retry": "^1.4.9",
    "@types/bun": "^1.3.1",
    "drizzle-kit": "^0.31.5"
  }
}
